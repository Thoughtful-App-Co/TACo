{
  "name": "thoughtful-app-co",
  "version": "0.1.0-beta",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:functions\" --names \"vite,api\" --prefix-colors \"cyan,magenta\"",
    "dev:vite": "vite",
    "dev:functions": "wrangler pages dev --proxy 3000 --port 8787 -b TACO_ENV=development",
    "db:setup": "npm run db:migrate:auth && npm run db:migrate:billing",
    "db:migrate:auth": "wrangler d1 execute taco-auth-staging --local --file=migrations/0001_auth_schema.sql",
    "db:migrate:billing": "wrangler d1 execute taco-billing-staging --local --file=migrations/0002_billing_schema.sql && wrangler d1 execute taco-billing-staging --local --file=migrations/0003_credits_schema.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && npm run db:setup",
    "build": "vite build && npm run copy-functions",
    "copy-functions": "cp -r functions dist/",
    "preview": "wrangler pages dev dist --port 8787",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@kobalte/core": "^0.13.11",
    "@solidjs/router": "^0.15.4",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "d3-shape": "^3.2.0",
    "date-fns": "^4.1.0",
    "dev": "^0.1.3",
    "idb-keyval": "^6.2.2",
    "jose": "^6.1.3",
    "keyword-extractor": "^0.0.28",
    "loglevel": "^1.9.2",
    "mammoth": "^1.8.0",
    "marked": "^17.0.1",
    "nanoid": "^5.1.6",
    "phosphor-solid": "^1.1.5",
    "resend": "^6.6.0",
    "solid-js": "^1.8.0",
    "solid-phosphor": "^1.0.16",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "unpdf": "^1.4.0",
    "web-push": "^3.6.7",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.4.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260101.0",
    "@eslint/js": "^9.39.1",
    "@types/d3-sankey": "^0.12.5",
    "@types/d3-shape": "^3.1.7",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-solid": "^0.14.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-solid": "^2.8.0",
    "workbox-window": "^7.4.0",
    "wrangler": "^4.51.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
